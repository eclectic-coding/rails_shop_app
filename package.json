    "@rails/webpacker": "5.2.1",
    "bootstrap": "^5.0.0-beta2",
    "popper.js": "^1.16.1",
    "turbolinks": "^5.2.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "husky": "5.1.3",
    "lint-staged": "^10.5.4",
    "webpack-dev-server": "^3.11.2"
  }
