    "@rails/webpacker": "5.4.0",
    "bootstrap": "^5.0.1",
    "popper.js": "^1.16.1",
    "turbolinks": "^5.2.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "husky": "4.3.8"
  }
